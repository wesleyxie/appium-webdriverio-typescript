{
  "compilerOptions": {
      "module": "commonjs",
      "target": "es2017",
      "moduleResolution": "node",
      "sourceMap": false,
      "declaration": false,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "removeComments": false,
      "noImplicitAny": false,
      "outDir": "output",
      "typeRoots": [ "./node_modules/@types", "./node_modules/@wdio" ],
      "types": ["node", "cucumber", "chai", "sync"]
  },
  "exclude": [
      "node_modules",
      "typescript"
  ]
}
